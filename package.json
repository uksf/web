{
  "name": "uksf",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uksf/website-frontend.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve && node server.js",
    "build": "ng build && node server.js",
    "lint": "ng lint",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:e2e": "node node_modules/@playwright/test/cli.js test",
    "test:e2e:ui": "node node_modules/@playwright/test/cli.js test --ui",
    "test:e2e:update-snapshots": "node node_modules/@playwright/test/cli.js test visual-baseline --update-snapshots",
    "storybook": "ng run uksf:storybook",
    "build-storybook": "ng run uksf:build-storybook",
    "test:storybook": "node node_modules/@playwright/test/cli.js test --config=playwright-storybook.config.ts"
  },
  "private": true,
  "packageManager": "bun@1.3.6",
  "dependencies": {
    "@ali-hm/angular-tree-component": "^19.0.1",
    "@angular/animations": "^19.2.18",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.18",
    "@angular/compiler": "^19.2.18",
    "@angular/core": "^19.2.18",
    "@angular/forms": "19.2.18",
    "@angular/material": "^19.2.19",
    "@angular/material-moment-adapter": "^19.2.19",
    "@angular/platform-browser": "^19.2.18",
    "@angular/platform-browser-dynamic": "19.2.18",
    "@angular/router": "19.2.18",
    "@auth0/angular-jwt": "^5.0.2",
    "@microsoft/signalr": "^6.0.0",
    "ansi-to-html": "^0.7.2",
    "chart.js": "^3.6.2",
    "eslint": "^8.4.1",
    "marked": "^15.0.0",
    "moment-timezone": "^0.5.35",
    "ngx-markdown": "^19.0.0",
    "ngx-permissions": "19.0.0",
    "ngx-quill": "24.0.5",
    "primeicons": "7.0.0",
    "primeng": "17.18.15",
    "quill": "^1.3.7",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.20",
    "@angular-devkit/architect": "^0.1902.20",
    "@angular-devkit/core": "^19.2.20",
    "@angular/cli": "^19.2.20",
    "@angular/compiler-cli": "^19.2.18",
    "@playwright/test": "^1.58.1",
    "@storybook/angular": "^8.4.0",
    "@storybook/addon-docs": "^8.4.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^16.11.12",
    "@types/quill": "^1.3.10",
    "dotenv": "^17.2.3",
    "http-server": "^14.1.1",
    "prettier": "^2.5.1",
    "storybook": "^8.4.0",
    "ts-node": "^10.4.0",
    "typescript": "~5.8.3",
    "vitest": "^4.0.18"
  },
  "prettier": {
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 200,
    "trailingComma": "none",
    "overrides": [
      {
        "files": [
          "*.html"
        ],
        "options": {
          "printWidth": 200
        }
      }
    ]
  }
}
