{
  "name": "uksf",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uksf/website-frontend.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve && node server.js",
    "build": "ng build && node server.js",
    "lint": "ng lint",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:update-snapshots": "playwright test visual-baseline --update-snapshots"
  },
  "private": true,
  "packageManager": "bun@1.3.6",
  "dependencies": {
    "@ali-hm/angular-tree-component": "12.0.6",
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/material-moment-adapter": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "15.2.10",
    "@angular/router": "15.2.10",
    "@auth0/angular-jwt": "^5.0.2",
    "@microsoft/signalr": "^6.0.0",
    "ansi-to-html": "^0.7.2",
    "chart.js": "^3.6.2",
    "core-js": "^3.19.3",
    "eslint": "^8.4.1",
    "moment-timezone": "^0.5.35",
    "ngx-markdown": "15",
    "ngx-permissions": "15",
    "ngx-quill": "21",
    "primeicons": "6",
    "primeng": "15",
    "quill": "^1.3.7",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular/cli": "^15.2.11",
    "@angular/compiler-cli": "^15.2.10",
    "@playwright/test": "^1.58.1",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^16.11.12",
    "@types/quill": "^1.3.10",
    "dotenv": "^17.2.3",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.9.5",
    "vitest": "^4.0.18"
  },
  "prettier": {
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 200,
    "trailingComma": "none",
    "overrides": [
      {
        "files": [
          "*.html"
        ],
        "options": {
          "printWidth": 200
        }
      }
    ]
  }
}